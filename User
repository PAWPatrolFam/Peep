import java.util.*;

public class Main {
    public static void main(String[] args) {
    Scanner scanner = new Scanner(System.in);
      
      int a = 123456;
      int b = 642135;
      int c = 013420;
      int user = scanner.nextInt();
      
      System.out.println("Enter your ID number: ");
      
      if (user == a) {
        System.out.println("Access Granted. Welcome user A");
      }else if (user == b) {
        System.out.println("Access Granted. Welcome user B");
      }else if (user == c) {
        System.out.println("Access Granted. Welcome user C");
      }else{
        System.out.println("Access Denied. Unauthorized User Detected");
      }
  }
}
